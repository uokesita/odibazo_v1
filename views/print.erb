<%
  @i = 0
  flickr = Flickr.new(File.join('config', 'flickr.yml'))
 
  flickr.photos.search(:user_id => "11807027@N03", :tags=>'PRINT').each do |o|
    @i = @i + 1
    @title = o.title
    @url = o.url
    %>
    <%= @title %>
    <br/>
    <img src="<%= @url %>"/>
    <br/>
    <%
  end

%>
<%= @i %>