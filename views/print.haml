- getPhotos('PRINT')
 
#section
  %div{ :id=>"galle"}
    %div{ :id=>"photos", :class=>"galleryview"}
      - @photo.each do |p|
        %div{ :class=>"panel"}
          #image
            %a{ :href=>"#{p[0][:url]}", :rel=>"lightbox-set"}
              %img{ :src=>"#{p[0][:urls]}", :style=>"max-height:310px;"}    